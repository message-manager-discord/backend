{
  "name": "message-manager-backend",
  "version": "1.0.0a",
  "description": "Backend for the message manager site",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "author": "AnotherCat <anothercat1259@gmail.com>",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node .",
    "dev": " tsc-watch --noClear --onSuccess \"node --experimental-specifier-resolution=node ./dist/index.js\"",
    "go": "npm run build && npm run start",
    "test": "tap --ts --test-ignore=dist",
    "migrate": "prisma migrate deploy",
    "generate": "prisma generate",
    "lint": "eslint  --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --write .",
    "format-check": "prettier --check ."
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "@sinclair/typebox": "^0.20.5",
    "axios": "^0.26.0",
    "detritus-client-rest": "^0.10.5",
    "discord-interactions": "^2.4.1",
    "env-schema": "^4.0.0",
    "fastify": "^3.22.0",
    "fastify-auth": "^1.1.0",
    "fastify-cookie": "^5.4.0",
    "fastify-cors": "^6.0.2",
    "fastify-error": "^0.3.1",
    "fastify-helmet": "^5.3.2",
    "fastify-plugin": "^3.0.0",
    "fastify-rate-limit": "^5.7.0",
    "fastify-raw-body": "^3.2.0",
    "fastify-swagger": "^4.13.0",
    "form-data-encoder": "^1.7.1",
    "formdata-node": "^4.3.2",
    "http-errors": "^1.8.0",
    "ioredis": "^4.28.2",
    "prisma-field-encryption": "^1.3.0",
    "prom-client": "^14.0.1",
    "redis-discord-cache": "github:message-manager-discord/redis-discord-cache",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/connect": "^3.4.35",
    "@types/http-errors": "^1.8.1",
    "@types/ioredis": "^4.28.5",
    "@types/node": "^17.0.17",
    "@types/tap": "^15.0.5",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "discord-api-types": "^0.27.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "2.6.1",
    "prisma": "^3.9.2",
    "tap": "^15.1.6",
    "ts-node": "^10.5.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.6.3"
  },
  "type": "module"
}
