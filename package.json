{
  "name": "message-manager-backend",
  "version": "2.3.2",
  "description": "Backend for the message manager site",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "author": "AnotherCat <anothercat1259@gmail.com>",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "tsc",
    "build:production": "tsc -p ./tsconfig.production.json",
    "start": "node --experimental-specifier-resolution=node .",
    "dev": " tsc-watch --noClear --onSuccess \"node --experimental-specifier-resolution=node ./dist/index.js\"",
    "go": "npm run build && npm run start",
    "test": "tap --ts --test-ignore=dist",
    "migrate": "prisma migrate deploy",
    "generate": "prisma generate",
    "lint": "eslint  --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@discordjs/rest": "^1.5.0",
    "@fastify/auth": "^4.2.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/rate-limit": "^7.6.0",
    "@fastify/swagger": "^7.6.1",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@prisma/client": "^4.8.0",
    "@sentry/integrations": "^7.29.0",
    "@sentry/node": "^7.29.0",
    "@sinclair/typebox": "^0.25.16",
    "axios": "^0.26.0",
    "discord-interactions": "^3.2.0",
    "env-schema": "^5.2.0",
    "fastify": "^4.11.0",
    "fastify-plugin": "^4.4.0",
    "fastify-raw-body": "^4.2.0",
    "form-data-encoder": "^2.1.4",
    "formdata-node": "^5.0.0",
    "fuse.js": "^6.6.2",
    "http-errors": "^2.0.0",
    "ioredis": "^5.2.4",
    "prisma-field-encryption": "^1.4.0",
    "prom-client": "^14.1.1",
    "redis-discord-cache": "github:message-manager-discord/redis-discord-cache",
    "stripe": "^11.9.1",
    "tslib": "^2.4.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/http-errors": "^2.0.1",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^17.0.45",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "discord-api-types": "^0.37.25",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.9.4"
  },
  "type": "module"
}
